<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jinnova.smartpad"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="14" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.jinnova.smartpad.android.feed.FeedActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.jinnova.smartpad.android.feed.FavoriteActivity"
             android:label="@string/app_name"></activity>
        <activity android:name="com.jinnova.smartpad.android.feed.MemberCardActivity"
             android:label="@string/app_name"></activity>
        <activity android:name="com.jinnova.smartpad.android.feed.OrdersActivity"
             android:label="@string/app_name"></activity>
        <activity android:name="com.jinnova.smartpad.android.feed.PromotionActivity"
             android:label="@string/app_name"></activity>
        <activity android:name="com.jinnova.smartpad.android.feed.StoreCatalogActivity"
             android:label="@string/app_name"></activity>
    </application>

</manifest>
